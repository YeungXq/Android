<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yxq.home">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Activity.MainActivity"
            android:label="首页">

        </activity>
        <activity
            android:name=".Activity.UserItemActivity.AboutUs"
            android:label="关于我们" />
        <activity
            android:name=".Activity.LoginActivity"
            android:label="Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activity.UserItemActivity.QueryInfo"
            android:label="用户信息" />
        <activity android:name=".Activity.RegisterActivity"
            android:label="Register"/>
        <activity android:name=".Activity.UserItemActivity.Setting"
            android:label="设置"/>
        <activity android:name=".Activity.UserItemActivity.Quit"/>
        <activity android:name=".Activity.ListItemActivity.ListItemActivity"
            android:label="文章内容"/>
    </application>

</manifest>